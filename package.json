{
  "name": "@yworks/migrate-yfiles-app",
  "version": "6.0.0",
  "description": "Semi-automatically migrate yFiles for HTML code to the latest version",
  "main": "src/index.js",
  "bin": {
    "migrate-yfiles-app": "src/cli.js"
  },
  "repository": "https://github.com/yWorks/migrate-yfiles-app",
  "scripts": {
    "test": "mocha test/**/test.js",
    "start": "node src"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "test/rename"
    ]
  },
  "files": [
    "doc",
    "mappings",
    "src",
    "transforms",
    "LICENSE",
    "README.md",
    "!*.tgz"
  ],
  "author": {
    "name": "yFiles for HTML team @ yWorks GmbH",
    "email": "yfileshtml@yworks.com"
  },
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "debug": "^4.3.4",
    "fs-extra": "^10.1.0",
    "jscodeshift": "^0.13.1",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/jscodeshift": "^0.11.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.2",
    "@types/yargs": "^17.0.10",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "typescript": "^4.7.4"
  }
}
