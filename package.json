{
  "name": "@yworks/migrate-yfiles-app",
  "version": "4.0.0",
  "description": "Semi-automatically migrate yFiles for HTML code to the latest version",
  "main": "src/index.js",
  "bin": {
    "migrate-yfiles-app": "src/cli.js"
  },
  "scripts": {
    "test": "mocha test/**/test.js",
    "start": "node src"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "test/rename"
    ]
  },
  "files": [
    "doc",
    "mappings",
    "src",
    "transforms",
    "LICENSE",
    "README.md",
    "!*.tgz"
  ],
  "author": {
    "name": "yFiles for HTML team @ yWorks GmbH",
    "email": "yfileshtml@yworks.com"
  },
  "license": "MIT",
  "dependencies": {
    "colors": "^1.4.0",
    "debug": "^4.1.1",
    "fs-extra": "^8.1.0",
    "js-codemod": "https://github.com/cpojer/js-codemod",
    "jscodeshift": "^0.7.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jscodeshift": "^0.6.3",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.1",
    "@types/yargs": "^15.0.3",
    "mocha": "^7.0.1",
    "prettier": "^1.19.1",
    "sinon": "^9.0.1",
    "typescript": "^3.8.3"
  }
}
