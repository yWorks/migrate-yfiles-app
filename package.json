{
  "name": "@yworks/migrate-yfiles-app",
  "description": "Semi-automatically migrate yFiles for HTML code to the latest version",
  "repository": "https://github.com/yWorks/migrate-yfiles-app",
  "author": "yWorks GmbH <yfileshtml@yworks.com>",
  "version": "30.0.0",
  "license": "MIT",
  "bin": "dist/cli.js",
  "engines": {
    "node": ">= 18"
  },
  "scripts": {
    "migrate": "node ./dist/cli.js",
    "migrate-internal": "tsx cli.ts --configPath=../../demos/tsconfig.json --target=yfiles",
    "migrate-playground": "tsx cli.ts --configPath=./playground/vite/tsconfig.json",
    "migrate-lib-snippets": "tsx cli.ts --configPath=../../../lib/src/snippets/tsconfig.json",
    "migrate-dguide-snippets": "tsx cli.ts --configPath=../../../dguide/src/examples/tsconfig.src.json",
    "build": "npx tsc",
    "test": "node --import tsx --test **/*.spec.ts"

  },
  "dependencies": {
    "kolorist": "^1.8.0",
    "lodash": "^4.17.21",
    "ts-morph": "^22.0.0"
  },
  "private": false,
  "type": "module",
  "files": [
    "README.md",
    "package.json",
    "dist"
  ]
}
